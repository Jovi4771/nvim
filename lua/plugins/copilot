return {
  -- copilot
  "zbirenbaum/copilot.lua",
  --event = "InsertEnter",
  config = function()
      require("copilot").setup({
        suggestion = {
          auto_trigger = true,
          debounce = 100,
          keymap = {
            accept = "<C-l>",
          },
        }
      })
  end
}
